        <TextView
            android:id="@+id/texty"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/blank" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/textView7"
